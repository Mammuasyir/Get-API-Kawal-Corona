<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.covidapp.fragment.BerandaFragment"
        android:label="@string/beranda"
        tools:layout="@layout/fragment_beranda" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.covidapp.fragment.ProvinsiFragment"
        android:label="@string/provinsi"
        tools:layout="@layout/fragment_provinsi" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.covidapp.fragment.InfoFragment"
        android:label="@string/info"
        tools:layout="@layout/fragment_info" />
</navigation>

